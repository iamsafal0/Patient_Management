{
    "version":2,
    "name":"patientmanagement",
    "rewrites": [
        { "source": "/(.*)", "destination": "/server.js" }
      ],
    "builds":[
        {
            "src":"server.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/server.js"
        }
    ]
}